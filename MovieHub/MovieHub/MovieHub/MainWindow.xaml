<Window x:Class="MovieHub.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MovieHub"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button x:Name="login" Content="Login" HorizontalAlignment="Left" Margin="218,230,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.557,0.315" Click="login_Click"/>
        <Button x:Name="register" Content="Register" HorizontalAlignment="Left" Margin="514,230,0,0" VerticalAlignment="Top" Click="register_Click"/>
        <Label Content="Welcome to MovieHub" HorizontalAlignment="Left" Margin="218,56,0,0" VerticalAlignment="Top" Width="346" FontWeight="Bold" FontSize="30"/>
    </Grid>
</Window>
